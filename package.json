{
  "name": "ngx-uploadx",
  "version": "1.0.8",
  "license": "MIT",
  "keywords": [
    "angular",
    "upload",
    "chunk"
  ],
  "scripts": {
    "start": "ng serve --aot",
    "build:app": "ng build",
    "test": "ng test",
    "test:once": "ng test --single-run",
    "lint": "ng lint",
    "e2e": "concurrently --kill-others -s 'first' 'npm run server' 'ng e2e'",
    "server": "node ./server/index.js",
    "server:dev": "cd ./server  && nodemon .",
    "prebuild": "node ./scripts/prebuild.js",
    "version": "npm run prebuild && git add src/uploadx/package.json && git commit -m 'version bump'",
    "postversion": "git push && git push --tags",
    "build": "ng-packagr -p ./src/uploadx/ng-package.json",
    "ci": "npm run lint && npm run test:once && npm run e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kukhariev/ngx-uploadx.git"
  },
  "homepage": "https://github.com/kukhariev/ngx-uploadx#readme",
  "dependencies": {
    "@angular/animations": "5.2.8",
    "@angular/common": "5.2.8",
    "@angular/compiler": "5.2.8",
    "@angular/core": "5.2.8",
    "@angular/forms": "5.2.8",
    "@angular/http": "5.2.8",
    "@angular/platform-browser": "5.2.8",
    "@angular/platform-browser-dynamic": "5.2.8",
    "@angular/router": "5.2.8",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.8",
    "@angular/language-service": "5.2.8",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.5.7",
    "body-parser": "^1.18.2",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "morgan": "^1.9.0",
    "ng-packagr": "^2.2.0",
    "nodemon": "^1.13.3",
    "protractor": "~5.1.2",
    "toxy": "^0.3.15",
    "ts-node": "~3.2.0",
    "tsickle": "^0.27.2",
    "tslint": "~5.7.0",
    "typescript": "2.6.2"
  }
}
